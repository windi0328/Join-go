                    location.href = '@Url.Action("LogOut", "Home")';
                }
            })
        }


        $(function () {
            //上方功能表下拉選單資料
            $.get('@Url.Action("MenuView", "Manage")', {}, function (data) {
                $('#navbarTogglerDemo02').html(data);
            });

             if ('@TempData["LoginMsg"]' != '') {
                  Swal.fire({
                    title: '@TempData["LoginMsg"]',
                    confirmButtonText: "確定"
                });
            }



            // 漢堡選單，點擊連結自動關閉漢堡選單
            $('#navbarNav .nav-link, .ShowLogin, .ShowRegister').on('click', function () {
                var navbarCollapse = bootstrap.Collapse.getOrCreateInstance($('#navbarNav')[0]);
                navbarCollapse.hide();
            });
        })
